    <formulas>
        <name>validation10_fr</name>
        <dataType>String</dataType>
        <expression>If(ISBLANK({!document_so.Date_VAT__c}),&quot;Datum zdanitelného plnění není vyplněno. &quot;,&quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>validation11_fr</name>
