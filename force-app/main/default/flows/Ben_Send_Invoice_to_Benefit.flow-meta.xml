    <decisions>
        <name>check_if_invoice_has_been_integrated</name>
        <label>check if invoice has been integrated</label>
        <locationX>578</locationX>
        <locationY>398</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>errors_exists</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>err_msg</leftValueReference>
                <operator>NotEqualTo</operator>
            </conditions>
            <connector>
                <targetReference>err_msg_scr</targetReference>
            </connector>
            <label>errors exists</label>
        </rules>
        <rules>
            <name>integrated</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>document_so.Benefit_Invoice_Number__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>document_so.Benefit_Invoice_Number__c</leftValueReference>
                <operator>NotEqualTo</operator>
            </conditions>
            <connector>
                <targetReference>integrated_invoice_scr</targetReference>
            </connector>
            <label>integrated</label>
        </rules>
        <rules>
            <name>not_integrated</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>document_so.Benefit_Invoice_Number__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>document_so.Benefit_Invoice_Number__c</leftValueReference>
                <operator>EqualTo</operator>
            </conditions>
            <connector>
                <targetReference>send_invoice_to_Benefit</targetReference>
            </connector>
            <label>not-integrated</label>
        </rules>
    </decisions>
