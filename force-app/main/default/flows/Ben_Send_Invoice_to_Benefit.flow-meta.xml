    </formulas>
    <formulas>
        <name>validation12_fr</name>
        <dataType>String</dataType>
        <expression>IF(TEXT({!document_so.CurrencyIsoCode}) = &quot;CZK&quot; &amp;&amp; ISBLANK({!document_so.Bank_Account__r.Bank_Account__c}), &quot;Číslo účtu není vyplněno. &quot;,&quot;&quot;)</expression>
    </formulas>
