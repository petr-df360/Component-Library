public class PassSelectedIdsToFlowVFController{
    
    public string[] SelectedContactIDs{get;set;}
    public string[] ContactIDs{get;set;}
    public string queryOutput{get;set;}
    public string myFilterId{get;set;}
    public string filterId = ApexPages.currentPage().getParameters().get('filterId');

    string objectName = 'Contact';

    public PassSelectedIdsToFlowVFController(ApexPages.StandardSetController listcontroller){
        
        FilterId = listcontroller.getFilterId();

        //system.debug('filterId is:'+filterId);
        //system.debug('myfilterId is:'+myfilterId);

        HttpRequest req = new HttpRequest();
        string baseUrl = URL.getSalesforceBaseUrl().toExternalForm();
        string endPoinURL = baseUrl+'/services/data/v50.0/sobjects/'+ objectName +'/listviews/'+ filterId +'/describe';
        req.setEndpoint(endPoinURL);
        req.setMethod('GET');
        req.setHeader('Authorization', 'Bearer ' + UserInfo.getSessionId());
        Http http = new Http();
        HttpResponse response = http.send(req);
        if( response.getStatusCode() == 200 ) {
            Map<String, Object> tokenResponse = (Map<String, Object>) JSON.deserializeUntyped(response.getBody());
            queryOutput = (String) tokenResponse.get('query');
            System.debug(queryOutput);
        }

        SelectedContactIDs = new string[]{};
        for(Contact con : (Contact[])listcontroller.getSelected()){
            SelectedContactIDs.add(con.Id);
        }

        List<Contact> contactList = Database.query(queryOutput);

        ContactIDs = new string[]{};
        for(Contact con : contactList){
            ContactIDs.add(con.Id);
        }
        system.debug(ContactIDs);
        

    }

    public static void getListviewFilters(String filterId, String objectName) {
	
        HttpRequest req = new HttpRequest();
        String baseUrl = URL.getSalesforceBaseUrl().toExternalForm();
        String endPoinURL = baseUrl+'/services/data/v50.0/sobjects/'+ objectName +'/listviews/'+ filterId +'/describe';
        req.setEndpoint(endPoinURL);
        req.setMethod('GET');
        req.setHeader('Authorization', 'Bearer ' + UserInfo.getSessionId());
        Http http = new Http();
        HttpResponse response = http.send(req);
        if( response.getStatusCode() == 200 ) {
            Map<String, Object> tokenResponse = (Map<String, Object>) JSON.deserializeUntyped(response.getBody());
            String query = (String) tokenResponse.get('query');
            System.debug(query);
        }
    } 

